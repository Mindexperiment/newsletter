{
    "name": "agpretto/newsletter",
    "description": "Newsletter package for Laravel applications",
    "keywords": [
        "newsletter",
        "laravel"
    ],
    "license": "MIT",
    "homepage": "https://github.com/Mindexperiment/newsletter",
    "support": {
        "issues": "https://github.com/Mindexperiment/newsletter/issues",
        "source": "https://github.com/Mindexperiment/newsletter"
    },
    "authors": [
        {
            "name": "Andrea Giuseppe Pretto",
            "email": "info@andreagiuseppe.com",
            "homepage": "https://andreagiuseppe.com"
        }
    ],
    "require": {},
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Agpretto\\Newsletter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Agpretto\\Newsletter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Agpretto\\Newsletter\\NewsletterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
